{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Gigabrain Docs",
  "colors": {
    "primary": "#22c55e",
    "light": "#4ade80",
    "dark": "#16a34a"
  },
  "fonts": {
    "body": {
      "family": "Avenir Next",
      "source": "https://gigabrain.gg/fonts/AvenirNext-Medium.woff2",
      "format": "woff2",
      "weight": 500
    },
    "heading": {
      "family": "Avenir Next",
      "source": "https://gigabrain.gg/fonts/AvenirNext-Bold.woff2",
      "format": "woff2",
      "weight": 700
    }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Platform",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "introduction",
              "problem-statement",
              "features",
              "architecture"
            ]
          },
          {
            "group": "Token & Economics",
            "pages": [
              "tokenomics",
              "roadmap"
            ]
          },
          {
            "group": "Help",
            "pages": [
              "faq"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://gigabrain.gg/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://t.me/gigabraingg",
          "icon": "telegram"
        },
        {
          "anchor": "X",
          "href": "https://x.com/gigabraingg",
          "icon": "X"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/gigabrain.png",
    "dark": "/logo/gigabrain.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://t.me/gigabraingg"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Launch Terminal",
      "href": "https://gigabrain.gg/login"
    }
  },
  "footer": {
    "socials": {
      "twitter": "https://x.com/gigabraingg",
      "telegram": "https://t.me/gigabraingg"
    }
  },
  "appearance": {
    "default": "dark",
    "strict": true
  }
}
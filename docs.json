          },
          {
            "group": "Token & Economics",
            "pages": [
              "token/tokenomics",
              "token/revenue",
              "token/roadmap"
            ]
          },
          {
            "group": "Gigabrain Fund",
            "pages": [
              "funds/alpha-fund",
              "funds/test-fund"
            ]
          },
          {
            "group": "Help",
            "pages": [
              "help/faq",
              "help/contact",
              "help/disclaimer"
            ]
          }
        ]
      },
      {
